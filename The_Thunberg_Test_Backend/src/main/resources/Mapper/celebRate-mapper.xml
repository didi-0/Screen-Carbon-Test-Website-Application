<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.uog.thunbergTest.mapper.CelebRateMapper">

    <insert id="updateAndInsert">

        REPLACE INTO celeb_rate

        (score, celeb_id, rating_date)

        VALUES (#{score}, #{celebId}, #{ratingDate})

    </insert>
</mapper>